{% extends 'base.html.twig'%}

{% block body %}
<div class="jumbotron text-center">
    <font face="montserrat">
    <h1>Nos annonces immobilières au Pays Basque et dans les Landes</h><br><br>
    </font>
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="https://www.maisons-sab.fr/wp-content/uploads/2018/07/22769710_346995245760904_5236855784682018523_o-1200x550.jpg" class="d-block w-100" alt="" >
        </div>
        <div class="carousel-item">
        <img src="https://www.maisons-sab.fr/wp-content/uploads/2018/07/14589737_146940222433075_641824564578872532_o-1200x550.jpg" class="d-block w-100" alt="maison2">
        </div>
        <div class="carousel-item">
        <img src="https://www.maisons-sab.fr/wp-content/uploads/2018/07/22712535_346995155760913_5664619341482767007_o-1200x550.jpg" class="d-block w-100" alt="">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
    </div>  
</div>
{% endblock %}




